{
  "expo": {
    "name": "Doggy World Care",
    "slug": "DoggyWorldCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "imageWidth": 500,
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.happy_coding.DoggyWorldCare"
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "extra": {},
    "plugins": ["expo-secure-store"]
  }
}
